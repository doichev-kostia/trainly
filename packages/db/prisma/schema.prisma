// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator kysely {
    provider = "prisma-kysely"
    output   = "../src"
	fileName = "types.ts"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id    String @id @default(uuid())
    email String @unique
    name  String
}
